{
  "name": "plugin-leg-us-ny",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/plugins/legislatures/us-ny/src",
  "tags": [
    "type:plugin",
    "scope:backend",
    "state:us-ny",
    "firebase:dep:server-app"
  ],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/plugins/legislatures/us-ny",
        "configFile": "packages/plugins/legislatures/us-ny/vite.config.ts"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "coverage/packages/plugins/legislatures/us-ny"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
