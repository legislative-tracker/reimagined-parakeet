{
  "name": "shared-data-models",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/shared/data-models/src",
  "projectType": "library",
  "tags": ["type:data-models", "scope:shared"],
  "// targets": "to see all targets run: nx show project shared-data-models --web",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/shared/data-models",
        "configFile": "packages/shared/data-models/vite.config.ts"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../../coverage/packages/shared/data-models"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
