{
  "name": "@reimagined-parakeet/plugin-leg-us-ny",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/plugins/legislatures/us-ny/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/plugins/legislatures/us-ny",
        "main": "libs/plugins/legislatures/us-ny/src/index.ts",
        "tsConfig": "libs/plugins/legislatures/us-ny/tsconfig.lib.json",
        "assets": [
          "libs/plugins/legislatures/us-ny/*.md"
        ],
        "format": [
          "cjs"
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/vitest:test",
      "outputs": [
        "{options.reportsDirectory}"
      ],
      "options": {
        "reportsDirectory": "../../../../coverage/libs/plugins/legislatures/us-ny"
      }
    }
  }
}
