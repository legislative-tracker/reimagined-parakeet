{
  "name": "@reimagined-parakeet/shared/data-models",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/shared/data-models/src",
  "tags": [
    "type:data-models",
    "scope:shared"
  ],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/shared/data-models",
        "main": "libs/shared/data-models/src/index.ts",
        "tsConfig": "libs/shared/data-models/tsconfig.lib.json",
        "assets": [
          "libs/shared/data-models/*.md"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "config": "libs/shared/data-models/vitest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
